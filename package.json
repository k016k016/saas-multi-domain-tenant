{
  "name": "saas-multi-domain-tenant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:p1": "playwright test e2e/tests/p1-baseline --project=p1-chromium",
    "test:e2e:p2": "playwright test e2e/tests/p2-members-audit --project=p2-chromium",
    "test:e2e:p1:ui": "playwright test e2e/tests/p1-baseline --project=p1-chromium --ui",
    "test:e2e:p2:ui": "playwright test e2e/tests/p2-members-audit --project=p2-chromium --ui",
    "setup:e2e": "tsx scripts/seed-test-user.ts",
    "seed:create-users": "tsx infra/supabase/seeds/create-auth-users.ts",
    "seed:run": "./infra/supabase/seeds/run-seeds.sh",
    "seed:all": "pnpm seed:create-users && pnpm seed:run"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^22.0.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "turbo": "^2.0.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@repo/db": "workspace:^"
  }
}
