      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.50.0"
  },
  "devDependencies": {
    "next": "^16.0.0",
    "typescript": "^5.7.0"
  }
}
